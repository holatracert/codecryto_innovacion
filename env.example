# Configuración del proyecto DIDs
NODE_ENV=development
PORT=3002

# MongoDB
MONGODB_URI=mongodb://admin:password123@localhost:27018/dids_db?authSource=admin

# PostgreSQL
POSTGRES_URI=postgresql://admin:password123@localhost:5433/dids_realtime

# Redis
REDIS_URI=redis://localhost:6380

# JWT
JWT_SECRET=your-super-secret-jwt-key-here-change-this-in-production

# Supabase
SUPABASE_URL=http://localhost:8001
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Configuración de la aplicación
APP_NAME=DIDs System
APP_VERSION=1.0.0
APP_DESCRIPTION=Sistema de Decentralized Identifiers con dispositivos móviles

# Configuración de notificaciones
NOTIFICATION_ENABLED=true
PUSH_NOTIFICATION_ENABLED=true

# Configuración de seguridad
CORS_ORIGIN=*
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Configuración de logs
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log

# Configuración de desarrollo
DEBUG=true
ENABLE_SWAGGER=true
